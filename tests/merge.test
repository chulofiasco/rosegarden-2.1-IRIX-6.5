#!../tclmidi
set mf [midimake]
midiconfig $mf {tracks 1}
midiput $mf 0 {0 NoteOff 0 1}
midiput $mf 0 {4 Note 0 1 2 1}
midiput $mf 0 {5 KeyPressure 0 1 2}

set mf2 [midimake]
midiconfig $mf2 {tracks 1} {division 480}
midiput $mf2 0 {16 NoteOff 0 1}
midiput $mf2 0 {24 Note 0 1 2 1}
midiput $mf2 0 {4 KeyPressure 0 1 2}

midimerge "$mf 0" "$mf2 0"

midirewind $mf
while {[set event [midiget $mf 0 next]] != "EOT"} {puts stdout $event}
